                <div class="profile-img">
                    <img src="/images/man.png" alt="" />
                </div>
                <ul class="list-container">
                    <li class="list-item">
                        <ion-icon
                            name="person-circle-outline"
                            class="icon"
                        ></ion-icon>
                        User Information
                    </li>
                    <li class="list-item">
                        <i class="fa-regular fa-message icon"></i>
                        Feedback
                    </li>
                    <li class="list-item">
                        <i class="fa-solid fa-file-contract icon"></i>
